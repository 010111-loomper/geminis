##########################################################################
##########################################################################
## ALGORITMOS

CheckSum() {
	
	pin=$PIN_P1
	pin=$(echo ${pin}+0|bc -l)
	acum=0
	
	while [ $pin -gt 0 ]; do
		acum=$(($acum + (3 * ($pin % 10))))
		pin=$(($pin / 10))
		acum=$(($acum + ($pin % 10)))
		pin=$(($pin / 10))
	done
	
	result=$(((10 - ($acum % 10)) % 10))
	PIN_P2=$(($result % 10000000))
}

